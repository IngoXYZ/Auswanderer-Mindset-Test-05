
# EmailJS Konfiguration (3 KOMBINIERTE TEMPLATES FÜR AUSWANDERER)
# Registriere dich auf emailjs.com und erstelle einen Service + 3 Templates

# Basis Configuration
NEXT_PUBLIC_EMAILJS_SERVICE_ID=service_auswanderer
NEXT_PUBLIC_EMAILJS_PUBLIC_KEY=your_public_key_here

# Kombinierte Templates (Kunde + Admin in einem Template)
NEXT_PUBLIC_EMAILJS_HIGH_TEMPLATE=template_auswanderer_high         # Hoher Score (75%+)
NEXT_PUBLIC_EMAILJS_MEDIUM_TEMPLATE=template_auswanderer_medium     # Mittlerer Score (60-74%)
NEXT_PUBLIC_EMAILJS_LOW_TEMPLATE=template_auswanderer_low           # Niedriger Score (<60%)

# Mailchimp Integration (für Newsletter-Automation)
NEXT_PUBLIC_MAILCHIMP_API_KEY=your_mailchimp_api_key_here
NEXT_PUBLIC_MAILCHIMP_SERVER_PREFIX=us1
NEXT_PUBLIC_MAILCHIMP_AUDIENCE_ID=your_audience_id_here

# Optional: Deine E-Mail für Empfang der Quiz-Ergebnisse
NEXT_PUBLIC_RECIPIENT_EMAIL=deine@email.com
